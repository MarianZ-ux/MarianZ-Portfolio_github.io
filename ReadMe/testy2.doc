/* * *Carousel container * * */
{box-sizing:border-box}
#carousel-section{
  position: relative;
  background-size: cover;
  background-repeat: no-repeat;
  background-attachment: fixed;
  padding: clamp(5px, 80vh, 50px) 0.3;
}
/* Slideshow container */
.carousel-inner img {
  object-fit: cover;
  object-position: center;
  height: 55vh;
  overflow: hidden;
  margin: auto;
}
.carousel-item:after {
  content:"";
  position: absolute;
  top:0;
  bottom:0;
  left:0;
  right:0;
  background: rgba(0,0,0,0.3);
}
.carousel-caption {
  z-index: 1;
} 
/* "next button" position right of img*/
.next {
  right: 0;
  border-radius: 3px 0 0 3px;
}
* html {
  scroll-behavior: auto !important;
}
/* Fading animation */
.fade {
  animation-name: fade;
  animation-duration: 1.5s;
}
@keyframes fade {
  from {opacity: .4}
  to {opacity: 1}
}
/* On hover-> black background color with lil bit transparancy */
.prev:hover, .next:hover {
  background-color: rgba(0,0,0,0.8);
  color:#C1A3A3;
}
/* Box sizing pseudo elements  ***/
img ::after, ::before {
  box-sizing: border-box;
}
.button:hover::before{
  min-height: 50px;
  width: 50px;
  color:#230903;
}
.button:hover::after {
  color: #717171;
  filter: brightness(75%);
}
.bi:hover::before {
  min-height: 50px;
  width: 50px;
  color:#230903;
}
.bi:hover::after {
  color: #717171;
  filter: brightness(75%);
}

#meet-me, #gallery, #contact {
  background-color: #776472;
  color: #EAEAEA;
}
#skills, #carousel-section {
    background-color: #2c374c;;
    color: #FFF2F1;
}
#nav-links, #nav-links {
